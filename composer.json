{
	"name": "jeroen/file-fetcher-stopwatch",
	"license": "GPL-2.0-or-later",
	"description": "Provides a Symfony Stopwatch decorator for file fetchers",
	"keywords": [
		"file_get_contents",
		"file fetcher",
		"filefetcher",
		"symfony",
		"stopwatch",
		"profile",
		"profiling"
	],
	"require": {
		"php": ">=8.1",
		"jeroen/file-fetcher": "~6.0|~5.0",
		"symfony/stopwatch": "~6.0|~5.0"
	},
	"require-dev": {
		"phpunit/phpunit": "^10.1.1",
		"squizlabs/php_codesniffer": "~3.7",
		"slevomat/coding-standard": "~8.10",
		"mediawiki/mediawiki-codesniffer": "~41.0",
		"phpstan/phpstan": "~1.10"
	},
	"autoload": {
		"psr-4": {
			"FileFetcher\\Stopwatch\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"FileFetcher\\Stopwatch\\Tests\\": "tests/"
		}
	},
	"config": {
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": false
		}
	}
}
