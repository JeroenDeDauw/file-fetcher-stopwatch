{
	"name": "jeroen/file-fetcher-stopwatch",
	"license": "GPL-2.0-or-later",
	"description": "Provides a Symfony Stopwatch decorator for file fetchers",
	"keywords": [
		"file_get_contents",
		"file fetcher",
		"filefetcher",
		"symfony",
		"stopwatch",
		"profile",
		"profiling"
	],
	"require": {
		"php": ">=7.1",
		"jeroen/file-fetcher": "~6.0|~5.0",
		"symfony/stopwatch": "~5.0|~4.2"
	},
	"require-dev": {
		"phpunit/phpunit": "~7.5",
		"ockcyp/covers-validator": "~1.0",
		"squizlabs/php_codesniffer": "~3.3",
		"slevomat/coding-standard": "~4.5",
		"mediawiki/mediawiki-codesniffer": "~23.0",
		"phpstan/phpstan": "~0.10.6"
	},
	"autoload": {
		"psr-4": {
			"FileFetcher\\Stopwatch\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"FileFetcher\\Stopwatch\\Tests\\": "tests/"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "1.0.x-dev"
		}
	}
}
